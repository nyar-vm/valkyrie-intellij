tagged Option<T> {
    /// No value.
    None,
    /// Some value of type `T`.
    Some(T),
}

tagged Either<L, R> {
    Left(T),
    Right(E),
}

tagged Validation<T, E> {
    Fine(T, [E]),
    Fail(E, [E]),
}

tagged Validation<T, E> {
    AA {
        a: A
    },
    BB {
        b: B
    },
}

